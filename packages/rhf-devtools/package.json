  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "react-hook-form": "^7.0.0"
  },
  "dependencies": {
    "@tanstack/devtools-event-client": "^0.3.5"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react": "^18.0.0",
