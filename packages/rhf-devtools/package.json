{
  "name": "@nourkodra/react-hook-form-devtools",
  "version": "1.1.0",
  "description": "A minimal devtools implementation for React Hook Form built using TanStack DevTools",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
